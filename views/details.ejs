        <script>
            $(function() {
                var location = $("#location").text().trim();
                $.ajax({
                    url: '/api/similarjobs/' + location,
                    type: 'GET',
                    success: function(data) {
                        data.forEach(function(job, index) {
                            var listitem = '<li class="list-group-item list-group-item-default">';
                            listitem = listitem + '<a href="/walkin/' + job._id + '" target="_blank">Walk-ins in ' + job.company + '</a></li>';
                            $("#jobList").append(listitem);
                        });
                    },
                    error: function(d) {
                        console.log('woops!'); //or whatever
                    }
                })
            });
