                                                                <li class="list-group-item list-group-item-default">
                                                                    <b class="list-group-item-heading">Job Description</b>
                                                                    <p class="list-group-item-text">
                                                                        <%= walkin.jobDescription %>
                                                                    </p>
                                                                </li>
                                                                <% } %>
                                                                    <% if (walkin.jobRequirements != '') { %>
                                                                        <li class="list-group-item list-group-item-default">
                                                                            <b class="list-group-item-heading">Job Requirements</b>
                                                                            <p class="list-group-item-text">
                                                                                <%= walkin.jobRequirements %>
                                                                            </p>
                                                                        </li>
                                                                        <% } %>
                                                                            <% if (walkin.candidateProfile != '') { %>
                                                                                <li class="list-group-item list-group-item-default">
                                                                                    <b class="list-group-item-heading">Candidate Profile</b>
                                                                                    <p class="list-group-item-text">
                                                                                        <%= walkin.candidateProfile %>
                                                                                    </p>
                                                                                </li>
                                                                                <% } %>
                                                                                    <% if (walkin.salary != '') { %>
                                                                                        <li class="list-group-item list-group-item-default">
                                                                                            <b class="list-group-item-heading">Salary</b>
                                                                                            <p class="list-group-item-text">
                                                                                                <%= walkin.salary %>
                                                                                            </p>
                                                                                        </li>
                                                                                        <% } %>
