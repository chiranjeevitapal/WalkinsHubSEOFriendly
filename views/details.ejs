                                <li class="list-group-item list-group-item-default">
                                    <b class="list-group-item-heading">Position</b>
                                    <p class="list-group-item-text">
                                        <%= walkin.position %>
                                    </p>
                                </li>
                                <% } %>
                                    <% if (walkin.location != '') { %>
                                        <li class="list-group-item list-group-item-default">
                                            <b class="list-group-item-heading">Location</b>
                                            <p class="list-group-item-text">
                                                <%= walkin.location %>
                                            </p>
                                        </li>
                                        <% } %>
                                            <% if (walkin.eligibility != '') { %>
                                                <li class="list-group-item list-group-item-default">
                                                    <b class="list-group-item-heading">Eligibility</b>
                                                    <p class="list-group-item-text">
                                                        <%= walkin.eligibility %>
                                                    </p>
                                                </li>
                                                <% } %>
